{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "TRANSFORMERS_CACHE": "/tmp/cache",
        "PYTHONUNBUFFERED": "1"
    }
}